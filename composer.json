{
    "name": "richardsalvaia/sped-common",
    "type": "library",
    "description": "sped-common Ã© parte do projeto NFePHP.",
    "keywords": [
        "nfe",
        "sped",
        "nfephp"
    ],
    "homepage": "https://github.com/nfephp-org/sped-common",
    "license": ["LGPL-3.0-or-later", "GPL-3.0-or-later", "MIT"],
    "authors": [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com.br",
            "homepage": "http://www.nfephp.org",
            "role": "Developer"
        },
        {
            "name": "Comunidade NFePHP",
            "homepage": "https://github.com/nfephp-org/sped-common/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-curl": "*",
        "ext-soap": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0",
        "neitanod/forceutf8": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^8.0",
        "scrutinizer/ocular": "^1.3",
        "phpstan/phpstan": "^0.12.99",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "NFePHP\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\Common\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-v5.0": "5.0-dev"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit",
        "phpcs": "./vendor/bin/phpcs src/ tests/",
        "phpcbf": "./vendor/bin/phpcbf src/ tests/",
        "stan": "phpstan analyse src/ tests/"
    }
}
